{
  "manifest_version": 2,
  "name": "WeRead Page Turner",
  "version": "1.0",
  "description": "使用鼠标滚轮在微信读书中翻页",
  "permissions": [
    "*://weread.qq.com/web/reader/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://weread.qq.com/web/reader/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "96": "icons/icon.png"
  }
}
